{
  "rewrites": [
    { "source": "/v/a/:path*", "destination": "https://cdn.cineveo.site/:path*" },
    { "source": "/v/b/:path*", "destination": "https://watch.brstream.cc/:path*" },
    { "source": "/v/c/:path*", "destination": "https://vods.playcnvs.com/:path*" },
    { "source": "/v/tv/:path*", "destination": "https://www2.embedtv.best/:path*" },
    { "source": "/b/rest/:path*", "destination": "https://mfcnkltcdvitxczjwoer.supabase.co/rest/:path*" },
    { "source": "/b/functions/:path*", "destination": "https://mfcnkltcdvitxczjwoer.supabase.co/functions/:path*" },
    { "source": "/b/realtime/:path*", "destination": "https://mfcnkltcdvitxczjwoer.supabase.co/realtime/:path*" },
    { "source": "/b/auth/:path*", "destination": "https://mfcnkltcdvitxczjwoer.supabase.co/auth/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/v/(.*)",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow" },
        { "key": "Cache-Control", "value": "private, no-store" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    },
    {
      "source": "/b/(.*)",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow" },
        { "key": "Cache-Control", "value": "private, no-store" }
      ]
    }
  ]
}
